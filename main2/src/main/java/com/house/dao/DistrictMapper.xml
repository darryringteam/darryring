<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<!-- namespace映射接口的全路径 -->
<mapper namespace="com.house.dao.DistrictMapper">
 <select id="queryDistrict" resultType="district">
     select * from hosdistrict
  </select>
  <select id="queryDistrictById" resultType="district">
      select * from hosdistrict where did=#{did}
  </select>
  
</mapper>